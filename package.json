{
  "name": "ui-thunder-link",
  "version": "1.0.0",
  "description": "Thunder Link UI APP (Real time chat app)",
  "author": "@IvaanTorres",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 80 --mode development",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest --coverage",
    "test:e2e": "cypress open",
    "test:unit": "vitest run --coverage",
    "coverage": "npm run test:unit && node sonar-project.js",
    "typecheck": "vue-tsc --noEmit",
    "start": "node server.js",
    "prepare": "husky install",
    "docker:build": "vite-ssg build --mode staging",
    "docker:local": "docker image prune -f && docker-compose down && docker-compose up --build --remove-orphans",
    "docker:local:not-api": "docker image prune -f && docker-compose down && docker-compose up --scale api=0 --build",
    "docker:local:ui": "docker image prune -f && docker-compose down && docker-compose up ui --build",
    "docker:prod": "docker image prune -f && docker-compose -f docker-compose.prod.yml down && docker-compose -f docker-compose.prod.yml --env-file .env.staging up --build",
    "docker:services": "docker image prune -f && docker-compose down && docker-compose up mongodb sonarqube",
    "docker:prune": "docker image prune -f"
  },
  "dependencies": {
    "@vueuse/core": "^7.7.0",
    "@vueuse/head": "^0.7.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "npm": "^8.5.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.11",
    "prism-theme-vars": "^0.2.2",
    "socket.io-client": "^4.4.1",
    "sonarqube-scanner": "^2.8.1",
    "vue": "^3.2.31",
    "vue-demi": "^0.12.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify-json/carbon": "^1.1.1",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@pinia/testing": "^0.0.11",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-attributify": "^0.26.4",
    "@unocss/preset-icons": "^0.26.4",
    "@unocss/preset-typography": "^0.26.4",
    "@unocss/preset-web-fonts": "^0.26.4",
    "@unocss/reset": "^0.26.4",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/test-utils": "^2.0.0-rc.18",
    "c8": "^7.11.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.0",
    "eslint": "^8.10.0",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.1",
    "husky": "^7.0.4",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.3",
    "pnpm": "^6.32.2",
    "typescript": "^4.5.5",
    "unocss": "^0.26.4",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.8.5",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-md": "^0.11.8",
    "vite-plugin-pages": "^0.21.0",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-ssg": "^0.17.10",
    "vitest": "^0.5.8",
    "vue-tsc": "^0.32.0"
  }
}
